    "url": "https://github.com/CharlyWelch/full-stack-image-gallery/issues"
  },
  "homepage": "https://github.com/CharlyWelch/full-stack-image-gallery#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "jest": "^23.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.1.5"
